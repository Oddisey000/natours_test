{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss assets/css/style_dev.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" assets/css/style_dev.css -o assets/css/style_dev.css",
    "compress:css": "node-sass assets/css/style_dev.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Oddisey000/natours_test.git"
  },
  "keywords": [
    "natours",
    "test",
    "css",
    "scss"
  ],
  "author": "Oddisey000",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Oddisey000/natours_test/issues"
  },
  "homepage": "https://github.com/Oddisey000/natours_test#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^6.1.3"
  }
}
